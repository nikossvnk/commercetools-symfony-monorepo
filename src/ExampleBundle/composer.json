{
    "name": "commercetools/symfony-example-bundle",
    "description": "Symfony ExampleBundle for Commercetools platform",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Commercetools\\Symfony\\ExampleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Commercetools\\Symfony\\ExampleBundle\\Tests\\": "Tests/"
        }
    },
    "require": {
      "php": "^7.1",
      "symfony/form": "^3.4 | ^4.0",
      "symfony/validator": "^3.4 | ^4.0",
      "commercetools/symfony-ctp-bundle": "dev-master",
      "commercetools/symfony-catalog-bundle": "dev-master",
      "commercetools/symfony-cart-bundle": "dev-master",
      "commercetools/symfony-customer-bundle": "dev-master",
      "commercetools/symfony-review-bundle": "dev-master",
      "commercetools/symfony-shopping-list-bundle": "dev-master",
      "commercetools/symfony-state-bundle": "dev-master"
    }
}
